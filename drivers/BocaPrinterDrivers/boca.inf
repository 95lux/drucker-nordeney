;
; Installer section(s) referenced above.
;
;Models 22/42 & 24/44 & 26/46 & Lemur Printers with FGL firmware and 200 DPI head
[fgl200]
CopyFiles=@boca200.dll,@boca200.GPD,FGLMON
DataSection=FGL_BIDI_DATA
DataFile=boca200.GPD
Include=NTPRINT.INF
Needs=UNIDRV.OEM

;Models 22/42 & 24/44 & 26/46  & Lemur Printers with FGL firmware and 300 DPI head
[fgl300]
CopyFiles=@boca300.dll,@boca300.GPD,FGLMON
DataSection=FGL_BIDI_DATA
DataFile=boca300.GPD
Include=NTPRINT.INF
Needs=UNIDRV.OEM

;Models 24/44 & 26/46  & Lemur Printers with FGL firmware and 600 DPI head
[fgl600]
CopyFiles=@boca600.dll,@boca600.GPD,FGLMON
DataSection=FGL_BIDI_DATA
DataFile=boca600.GPD
Include=NTPRINT.INF
Needs=UNIDRV.OEM

;Models 22/42 & 24/44 & 26/46  & Lemur Printers with PCL emulation firmware and 300 DPI head
[pcl300]
CopyFiles=@pcl300.dll,@pcl300.GPD
DataSection=UNIDRV_DATA
DataFile=pcl300.GPD
Include=NTPRINT.INF
Needs=UNIDRV.OEM,TTFSUB.OEM

[FGLMON]
ddkfglmon.dll,,,0x00000020		;Language Monitor
msvcr120.dll,,,0x00000020		;needed when using VS 2013 for drivers

[FGL_BIDI_DATA]
DriverFile=UNIDRV.DLL
ConfigFile=UNIDRVUI.DLL
HelpFile=UNIDRV.HLP
LanguageMonitor=%FGL_MONITOR%

[DestinationDirs]
DefaultDestDir=66000
FGLMON=66002

;;; Remove 2021Nov19 due no i386 section is causing INF2CAT errors
;;;[SourceDisksNames.x86]
;;;1 = %disk1%,,,\i386
;;; End Remove 2021Nov19

[SourceDisksNames.amd64]
1 = %disk1%,,,\amd64

;;; Monitor this section in case it starts producing INF2CAT errors
[SourceDisksNames.ia64]
1 = %disk1%,,,\ia64

[SourceDisksFiles]
boca200.dll = 1
boca200.GPD = 1
boca300.dll = 1
boca300.GPD = 1
boca600.dll = 1
boca600.GPD = 1
pcl300.dll = 1
pcl300.GPD = 1
ddkfglmon.dll=1		;Language Monitor
msvcr120.dll=1		;needed when using VS 2013 for drivers

[Strings]
PrinterPortType="FGL Port Monitor"
PrinterClassName="Printers"
BOCA="Boca Systems Inc."
disk1="Boca Printer Driver Setup Disk"
FGL_MONITOR="FGL Language Monitor,ddkfglmon.dll"
