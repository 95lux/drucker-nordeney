    {
        *Name: "896KB"
        *MemoryConfigKB: PAIR(896, 896)
    }
    *Option: 2560KB
    {
        *Name: "2560KB"
        *MemoryConfigKB: PAIR(2560, 2560)
    }
}

*Feature: PAPER_HANDLING
{
    *rcNameID: =OUTPUTBIN_DISPLAY
    *%Name: = PAPER_HANDLING
    *DefaultOption: Normal_Mode
    *Option: PLAIN_PAPER_DISPLAY
    {
        *Name: "Normal Mode"
        *Command: CmdFF { *Cmd : "%<p>" }
        *Command: CmdSelect
        {
        *Order: PAGE_FINISH.1
        *Cmd: ""
        }
    }
    *%Option: GLOSSY_PAPER_DISPLAY
    *Option: NOCUT
    {
        *Name: "Disable Cutter"
        *Command: CmdFF { *Cmd : "%<q>" }
        *Command: CmdSelect
        {
        *Order: PAGE_FINISH.1
        *Cmd: ""
        }
    }
        *Option: MATTE_PAPER_DISPLAY
    {
        *Name: "Cut last ticket of job only"
        *Command: CmdFF { *Cmd : "%<q>" }
        *Command: CmdSelect
        {
        *Order: JOB_FINISH.1
        *Cmd: "%<FC>"
        }
    }
}

*RotateCoordinate?: FALSE
*RotateRaster?: TRUE
*RotateFont?: TRUE
