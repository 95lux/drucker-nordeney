    *Option: 11X17
 {
    *rcNameID: =RCID_DMPAPER_SYSTEM_NAME
                *PrintableArea: PAIR(2200, 3400)
                *PrintableOrigin: PAIR(2,0)
                *CursorOrigin: PAIR(0, 0)
       *CenterPrintable?: FALSE
}

    *Option: LETTER
    {
        *rcNameID: =RCID_DMPAPER_SYSTEM_NAME
        *PageProtectMem: 4109
        *switch: Orientation
        {
            *case: PORTRAIT
            {
                *PrintableArea: PAIR(1700, 2200)
                *PrintableOrigin: PAIR(2, 0)
                *CursorOrigin: PAIR(0, 0)
            }
            *case: LANDSCAPE_CC90
            *%case: LANDSCAPE_CC270
            {
                *PrintableArea: PAIR(1700, 2200)
                *PrintableOrigin: PAIR(2, 0)
                *CursorOrigin: PAIR(0, 0)
            }
        }
    }


